<SageTVPlugin>
       <Name>SageAlert (common)</Name>
       <Identifier>sagealert-common</Identifier>
       <Description>Common classes required for SageAlert and for other plugins to add passive SageAlert support. (Dependency only, most users should not directly install this plugin.)</Description>
       <Author>Slugger,jusjoken</Author>
       <CreationDate>2010.05.29</CreationDate>
       <ModificationDate>@@MOD_DATE@@</ModificationDate>
       <Version beta="true">@@VER_NUM@@.@@BLD_NUM@@</Version>
       <ResourcePath>plugins/sagealert-common</ResourcePath>
       <Webpage>http://sagetv-addons.googlecode.com</Webpage>
       <PluginType>Library</PluginType>
       <Dependency>
       	<JVM/>
       	<MinVersion>1.8</MinVersion>
       </Dependency>
       <Package>
               <PackageType>JAR</PackageType>
               <Location>https://github.com/OpenSageTV/sagetv-plugin-repo/releases/download/1/sagealert-common-@@VER_NUM@@.@@BLD_NUM@@.zip</Location>
               <MD5>@@COMMON_MD5@@</MD5>
               <Overwrite>true</Overwrite> <!-- Defaults to true -->
       </Package>
</SageTVPlugin>
